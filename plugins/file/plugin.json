{
  "$schema": "https://fractary.com/schemas/plugin-manifest-v1.json",
  "name": "@fractary/file-plugin",
  "version": "2.0.0",
  "description": "File storage primitive - Universal file operations across Local, R2, S3, GCS, and Google Drive",
  "author": "Fractary Team",
  "homepage": "https://github.com/fractary/plugins",
  "repository": "https://github.com/fractary/plugins",
  "license": "MIT",
  "tags": ["file", "storage", "r2", "s3", "gcs", "gdrive", "official", "primitive"],

  "agents": [
    {
      "name": "file-manager",
      "version": "2.0.0",
      "description": "Universal file storage agent - routes file operations to specialized storage handlers",
