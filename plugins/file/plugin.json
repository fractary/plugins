      "provider": "anthropic",
      "model": "claude-opus-4-5"
    },
    "permissions": {
      "read": ["**/*.md", "**/*.yaml", "**/*.json", ".fractary/**"],
      "write": [".fractary/**", "uploads/**"],
      "execute": ["bash"]
    }
  }
